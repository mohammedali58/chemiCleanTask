{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "DBConString": "Server=.;Database = ChemiClean_Dev;Trusted_Connection=True"
  },

  "SwaggerBasePath": "",
  "AllowedHosts": "*",
  "AllowedOrigins": [
    "http://localhost:4200"
  ],
  "WriteTo": [
    {
      "Name": "File",
      "Args": {
        "path": "C:\\ChemiClean\\",
        "rollingInterval": "Day",
        "retainedFileCountLimit": 7
      }
    }
  ],
  "FilesPaths": {
    "BaseUrl": "D:\\Download"
    //,"ServerUrl": "https://xxx.xxx.xxx.xxx/chemiClean/Documents/"
  }

}
